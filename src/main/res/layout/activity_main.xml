<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="newera.EliJ.MainActivity">

    <newera.EliJ.ui.view.CImageView
            android:id="@+id/cImageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <newera.EliJ.ui.view.StartupView android:layout_width="match_parent" android:layout_height="match_parent"
                                     android:id="@+id/startupView"
/>

    <newera.EliJ.ui.view.CircleMenu
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/filterMenu"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <newera.EliJ.ui.view.CircleMenu
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/sysMenu"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
