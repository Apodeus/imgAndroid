<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="newera.myapplication.MainActivity">

    <newera.myapplication.ui.view.CImageView
            android:id="@+id/cImageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <newera.myapplication.ui.view.StartupView android:layout_width="match_parent" android:layout_height="match_parent"
    android:id="@+id/startupView"/>

    <newera.myapplication.ui.view.CircleMenu
        android:layout_width="320dp"
        android:layout_height="320dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="normal" app:srcCompat="@color/colorAccent"
        android:id="@+id/floatingActionButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
